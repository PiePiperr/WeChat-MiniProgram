.header {
  display: flex;
  background-color: #F3F3F3;
  border-bottom: 1px solid #e9e9e9;
  padding: 40rpx 0 40rpx 40rpx;
  font-size: 40rpx;
  font-weight: bold;
}

.content {
  display: flex;
  padding: 0 20rpx;
  background-color: white
}

.flexitem1 {
  width: 100%;
  padding: 40rpx 0;
  color: #212529;
  border-bottom: 1px solid #fff0ec;
}

.flexitem1 text {
  font-size: 25rpx;
  margin: 0 15rpx;
}

.none {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: absolute;
  top: 35%;
  width: 100%;
  color: #212529
}

.none image {
  width: 244rpx;
  height: 200rpx;
  margin-bottom: 20rpx  
}

.search{
  margin-top: 40rpx;
  padding: 10rpx 50rpx;
  font-size: 40rpx;
  background-color:white;
  border-radius: 10px;
  border: solid 1px gray
}